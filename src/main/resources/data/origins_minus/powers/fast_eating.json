  "charge_while_eating": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_res",
      "change": 2
    },
    "interval": 2,
    "condition": {
      "type": "origins:using_item",
      "item_condition": {
        "type": "origins:or",
        "conditions": [
          {
            "type": "origins:ingredient",
            "ingredient": {
              "tag": "origins_minus:fast_eating_1"
            }
          },
          {
            "type": "origins:ingredient",
            "ingredient": {
              "tag": "origins_minus:fast_eating_2"
